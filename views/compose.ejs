<%- include("partials/header") -%>

<div class="comp container-fluid">

<h1>Compose</h1>

<form  action="/" method="post">

<div class="form-group">
<label for="title"><h3>Title</h3></label>
<input type="text" name="Title" class="form-control" id="title" autocomplete="off">
</div>

<div class="form-group">
  <label for="content"><h3>Your Content Here</h3></label>
  <textarea type="text" name="Content" class="form-control" id="content" rows="4" autocomplete="off"></textarea>
</div>

<div class="form-group">
<label for="auth"><h3>Name of Author</h3></label>
<input type="text" name="author" class="form-control" id="auth">
</div>

<div class="form-group">
<label for="College"><h3>Name of College / Company / Center</h3></label>
<input type="text" name="CollegeName" class="form-control" id="College">
</div>

<button type="submit" name="submit" class="btn btn-primary btn-lg">Publish</button>
</form>



 
<div class="footer-padding">
<footer><center>Made with ❤️ for Krishna</center></footer>
</div>

</div>
</body>
</html>
